               <assignment id="jkwsxebf">
                  <title>Set Job Search  Query</title>
                  <operation source="formula" to="temp.tm_job_query">
                     <expression language="XQuery">let $status_cond := switch ($temp.tmp_search_logging_level)
    case "Running" return 
            &lt;condition&gt;STATUS__c = '1'&lt;/condition&gt;
    case "Failed" return 
            &lt;condition&gt;STATUS__c = '2'&lt;/condition&gt;
    case "Completed"  return 
            &lt;condition&gt;STATUS__c = '3'&lt;/condition&gt;
    default return () 
let $query := &lt;query&gt;
  &lt;condition&gt;Id = '{$temp.tmp_search_id }'&lt;/condition&gt;
  &lt;condition&gt;Name  LIKE '%{$temp.tmp_search_job_name }%'&lt;/condition&gt;
  &lt;condition&gt;ENVIRONMENT__c  LIKE '%{$temp.tmp_search_job_environment }%'&lt;/condition&gt;
  &lt;condition&gt;JOB_PROCESS_ID__c = '{$temp.tmp_search_job_pid}'&lt;/condition&gt;
  &lt;condition&gt;START_DATE__c &amp;gt;= {$temp.tmp_search_job_start_from }&lt;/condition&gt;
  &lt;condition&gt;START_DATE__c &amp;lt;= {$temp.tmp_search_job_start_to }&lt;/condition&gt;
  {$status_cond}
 &lt;/query&gt;
 
let $conditions := for $condition in $query/condition/text()
  where (:Filter conditions that had empty values:)
  not (contains($condition,"''")) and not(contains($condition,"%%"))
  return $condition

let $limit_part := if (empty($temp.tmp_search_job_limit )) then () else concat("LIMIT ",$temp.tmp_search_job_limit)
let $order_by_part := '&amp;#10; Order By START_DATE__c DESC &amp;#10;'



return
concat(string-join($conditions, "&amp;#10; AND ")," ",$order_by_part,$limit_part)</expression>
                  </operation>
