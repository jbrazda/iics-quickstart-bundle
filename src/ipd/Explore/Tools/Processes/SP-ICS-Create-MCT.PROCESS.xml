   {$input.in_parameters}
   &lt;name&gt;{$input.in_mapping_task_name}&lt;/name&gt;
   &lt;agentId&gt;{$temp.tmp_agent_id}&lt;/agentId&gt;
   &lt;mappingId&gt;{$temp.tmp_mapping_id}&lt;/mappingId&gt;
&lt;/CreateMappingTaskRequest&gt;
</expression>
