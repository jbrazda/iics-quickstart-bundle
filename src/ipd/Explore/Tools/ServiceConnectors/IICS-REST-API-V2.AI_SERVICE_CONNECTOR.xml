                        <options>
                           <option name="referenceTo">agentDetail</option>
                        </options>
                        <xquery>{let $payload := $RESTResponse/*:payload/text()
let $payloadXML := try {
        util:parseJSON($payload)
        } catch * {
            ()
        }
return
for $item in $payloadXML
return
&lt;agent&gt;
	{$item/*}
&lt;/agent&gt;}</xquery>
                     </field>
