               <create id="jp8dxl0b">
                  <title>IC_CONFIGURATION</title>
                  <entityName>IPaaS-Configuration-DB:IC_CONFIGURATION</entityName>
                  <createInput>
                     <parameter id="jp8g7j8d" name="PKEY" source="field" updatable="true">temp.tmp_property_key</parameter>
                     <parameter id="jp8g7j8e" name="PVALUE" source="field" updatable="true">temp.tmp_property_value</parameter>
                  </createInput>
                  <actions>
                     <action alt="null" id="jp8dxl0b_jp8dxl1l" targetScreen="jp8dxl1l">
                        <text>Continue</text>
                     </action>
                  </actions>
               </create>
               <assignment id="k2m6wduw">
                  <title>Initialize</title>
                  <operation source="formula" to="temp.tmp_config_dataSource">
                     <expression language="XQuery">if (util:resolveURN('urn:ic_configuration:dataSource')= 'urn:ic_configuration:dataSource' ) then 'DS1' else util:resolveURN('urn:ic_configuration:dataSource')</expression>
                  </operation>
                  <operation source="field" to="temp.tmp_key_search">input.in_property_filter</operation>
                  <actions>
                     <action alt="null" id="k2m6wduw_jp8dxku3" targetScreen="jp8dxku3">
                        <text>Continue</text>
                     </action>
                  </actions>
               </assignment>
               <assignment id="jp8dxku3">
                  <title>Set Query</title>
                  <operation source="formula" to="temp.tmp_query">
                     <expression language="XQuery">let $conditions := for $filter in tokenize($temp.tmp_key_search,',')
	let $condition := if (contains($temp.tmp_key_search,"%")) then &lt;condition&gt;PKEY LIKE '{$filter}'&lt;/condition&gt;/text()
    	else &lt;condition&gt;PKEY LIKE '%{$filter}%'&lt;/condition&gt;/text()
    return 
    $condition
return 
concat(string-join($conditions,' or '),' LIMIT ',$temp.tmp_query_limit)</expression>
                  </operation>
                  <actions>
                     <action alt="null" id="jp8dxku3_jp8dxkvq" targetScreen="jp8dxkvq">
                        <text>Continue</text>
                     </action>
                  </actions>
               </assignment>
               <assignment id="jp8dxkvq">
                  <title>Query Configurations</title>
                  <operation source="list" to="temp.tmp_configurations">
                     <query advanceQuery="false" from="IPaaS-Configuration-DB:IC_CONFIGURATION">
                        <where>{$temp.tmp_query} </where>
                     </query>
                  </operation>
                  <actions>
                     <action alt="null" id="jp8dxkvq_jp8dxkub" targetScreen="jp8dxkub">
                        <text>Continue</text>
                     </action>
                  </actions>
               </assignment>
               <assignment id="jp8dxl5a">
                  <title>Read Properties</title>
                  <operation source="field" to="temp.tmp_property_key">temp.tmp_selected_configuration[1]/PKEY</operation>
                  <operation source="field" to="temp.tmp_property_value">temp.tmp_selected_configuration[1]/PVALUE</operation>
                  <actions>
                     <action alt="null" id="jp8dxl5a_jp8dxkv0" targetScreen="jp8dxkv0">
                        <text>Continue</text>
                     </action>
                  </actions>
               </assignment>
               <assignment id="kzstkt3u">
                  <title>Convert to CSV</title>
                  <operation source="formula" to="temp.tmp_export_data">
                     <expression language="XQuery">let $export_items := for $item in $temp.tmp_selected_configurations 
				   let $key := replace($item/PKEY/text(),'[&amp;#10;&amp;#10;]','/n')
                   let $value := replace($item/PVALUE/text(),'[&amp;#10;&amp;#10;]','/n')
                   order by $item/PKEY
                   return
				   concat('"',$key,'","',$value,'"')
let $header := 'PKEY,PVALUE'
return string-join(($header,$export_items), '&amp;#10;')
</expression>
                  </operation>
                  <actions>
                     <action alt="null" id="kzstkt3u_kzstkt2g" targetScreen="kzstkt2g">
                        <text>Continue</text>
                     </action>
                  </actions>
               </assignment>
               <assignment id="kzstkt4m">
                  <title>Convert to SQL</title>
                  <operation source="formula" to="temp.tmp_export_data">
                     <expression language="XQuery">let $export_items := for $item in $temp.tmp_selected_configurations 
				   let $key :=  $item/PKEY/text()
                   let $value := $item/PVALUE/text()
                   order by $item/PKEY
                   return
				   concat("INSERT INTO IC_CONFIGURATION (PKEY, PVALUE) VALUES ('",$key,"','",$value,"')")
return string-join($export_items, '&amp;#10;')
</expression>
                  </operation>
                  <actions>
                     <action alt="null" id="kzstkt4m_kzstkt2g" targetScreen="kzstkt2g">
                        <text>Continue</text>
                     </action>
                  </actions>
               </assignment>
               <assignment id="kzstkt51">
                  <title>Convert to XML</title>
                  <operation source="formula" to="temp.tmp_export_data">
                     <expression language="XQuery">let $saxon_params :=&lt;xsl:output xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
                                xmlns:saxon="http://saxon.sf.net/"
                                method="xml"
                                omit-xml-declaration="yes"
                                indent="yes"
                                saxon:indent-spaces="4"/&gt;
let $xml := &lt;configuration&gt;
	{for $item in $temp.tmp_selected_configurations
     order by $item/PKEY
     return
     &lt;item&gt;
     	{$item/*}
     &lt;/item&gt;
    }
    
&lt;/configuration&gt;
return 
saxon:serialize(util:simplifyXml( $xml), $saxon_params)
</expression>
                  </operation>
                  <actions>
                     <action alt="null" id="kzstkt51_kzstkt2g" targetScreen="kzstkt2g">
                        <text>Continue</text>
                     </action>
                  </actions>
               </assignment>
