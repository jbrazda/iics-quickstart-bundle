                                method="xml"
                                omit-xml-declaration="yes"
                                indent="yes"
                                saxon:indent-spaces="4"/&gt;
let $xml := &lt;root&gt;
{util:parseJSON($input.in_payload )}
&lt;/root&gt;
return 
saxon:serialize($xml, $saxon_params)
</expression>
